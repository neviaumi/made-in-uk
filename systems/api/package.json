    "collectCoverageFrom": [
      "<rootDir>/**/*.ts",
      "!<rootDir>/**/*.spec.ts",
      "!<rootDir>/seeder/*.ts"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "injectGlobals": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "testTimeout": 300000,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
